@import "../_variables"
@import "/bower_components/bootstrap-stylus/stylus/mixins"

.small-photo
  width 40px
  height 40px
  background #eee
  display inline-block
  margin 3px

.photo-wrap
  opacity 0
  display inline-block
  transition(all 1s cubic-bezier(0.23, 1, 0.32, 1))

  &.loaded
    opacity 1